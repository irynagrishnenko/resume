<main class='p-4'>
  <div>
    <div class='row'>
      {{#with navigation}}
        {{#each links}}
          <div class='col-2'>
            <a
              class='btn btn-outline-link text-primary'
              href='{{href}}'
            >{{text}}</a>
          </div>
        {{/each}}
        <div class='col-2'>
          {{#with button}}
            <a
              class='btn btn-primary'
              href='{{href}}'
            >{{text}}</a>
          {{/with}}
        </div>
      {{/with}}
    </div>
  </div>
  <hr />
  <br />
  <div>
    {{#with header}}
      <h1>{{title}}</h1>
      <p>{{description}}</p>
      {{#with button}}
        <a
          class='btn btn-primary'
          href='{{link}}'
        >{{text}}</a>
      {{/with}}
    {{/with}}
  </div>
  <br />
  <hr />
  <br />
  <div>
    <h2>{{city.title}}</h2>
    <br />
    <div class='row'>
      {{#each city.cards}}
        <div class='col-5 gx-2 gy-4'>
          <div class='card'>
            <img
              class='card-img-top'
              src='{{picture.src}}'
              alt='{{picture.alt}}'
            />
            <div class='card-body'>
              <p
                class='card-title text-capitalize'
              >{{title}}</p>
              <p>{{description}}</p>
              <a
                href='{{button.href}}'
                class='btn btn-primary'
              >{{button.text}}</a>
            </div>
          </div>
        </div>
      {{/each}}
    </div>
  </div>
  <br />
  <hr />
  <br />
  <div>
    {{#with winner}}
      <h2>{{title}}</h2>
      <br />
      {{#with table}}
        <table class='table'>
          <thead>
            <tr>
              {{#each head}}
                <th>
                  {{this}}
                </th>
              {{/each}}
            </tr>
          </thead>
          <tbody>
            {{#each user}}
              <tr>
                <th>{{place}}</th>
                <td>{{name}}</td>
                <td>{{surname}}</td>
                <td>{{nickname}}</td>
              </tr>
            {{/each}}
          </tbody>
        </table>
      {{/with}}
    {{/with}}
  </div>
  <br />
  <hr />
  <br />
  <div>
    {{#with gallery}}
      <h2>{{title}}</h2>
      <br />
      <div class='row g-2'>
        {{#each cards}}
          <div class='col-4'>
            <div class='card'>
              <div class='card-header'>{{name}}</div>
              <ul class='list-group list-group-flush'>
                {{#each items}}
                  <li class='list-group-item'>{{this}}</li>
                {{/each}}
              </ul>
            </div>
          </div>
        {{/each}}
      </div>
    {{/with}}
  </div>
  <br />
  <hr />
  <br />
  <div>
    {{#with relaxion}}
      <div class='display-3'>{{title}}</div>
      <br />
      <div class='row'>
        {{#each cards}}
          <div class='col-5 gx-2 gy-4'>
            <div class='card text-bg-secondary'>
              <div class='card-header'>{{header}}</div>
              <div class='card-body'>
                <div class='card-title'>{{title}}</div>
                <p>{{description}}</p>
              </div>
            </div>
          </div>
        {{/each}}
      </div>
    {{/with}}
  </div>
  <br />
  <hr />
  <br />
  <div>
    {{#with rest}}
      <div class='row'>
        {{#each cards}}
          <div class='col-6 gx-2'>
            <div class='card text-center'>
              <div class='card-header'>{{header}}</div>
              <div class='card-body'>
                <h5 class='card-title'>{{title}}</h5>
                <p>{{description}}</p>
                <button
                  class='btn btn-primary'
                >{{button}}</button>
              </div>
              <div class='card-footer'>{{footer}}</div>
            </div>
          </div>
        {{/each}}
      </div>
    {{/with}}
  </div>
  <br />
  <hr />
  <br />
  <div>
    {{#with proposition}}
      <div class='display-2'>{{title}}</div>
      <br />
      <div class='row'>
        {{#each card}}
          <div class='col-5 g-2'>
            <div class='card text-bg-warning'>
              <div class='card-header'>{{header}}</div>
              <div class='card-body'>
                <h5 class='card-title'>{{title}}</h5>
                <p>{{description}}</p>
              </div>
            </div>
          </div>
        {{/each}}
      </div>
    {{/with}}
  </div>
  <br />
  <hr />
  <div>
    {{#with footer}}
      <h5>{{title}}</h5>
      <br />
      <div class='row'>
        <div class='col-3'>
          {{#each navigation}}
            <a
              class='btn btn-outline-link text-primary'
              href='{{href}}'
            >{{text}}</a>
            <br />
          {{/each}}
        </div>
        <div class='col-3'>
          {{#each socialMedia}}
            <a
              class='btn btn-outline-dark btn-sm'
              href='{{link}}'
            >{{name}}</a>
            <br />
            <br />
          {{/each}}
        </div>
        <div class='col-3'>
          {{#each partners}}
            <a
              class='btn btn-outline-info btn-sm'
              href='{{link}}'
            >{{name}}</a>
            <br />
            <br />
          {{/each}}
        </div>
      </div>
    {{/with}}
  </div>
</main>