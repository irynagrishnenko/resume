<div class='p-4'>
  <h1 class='text-center mb-4'>{{name}}</h1>
  <main>
    <h2 class='text-center mb-3'>Users</h2>
    <section>
      <ol>
        <div class='row'>
          {{#each users}}
            <div class='col-6'>
              <li>
                id:
                {{id}}
                <br />
                name:
                {{name}}
                <br />
                gender:
                {{gender}}
                <br />
                age:
                {{age}}
                <br />
                <br />
                <h5>Friends</h5>
                <ol>
                  {{#each friends}}
                    <li>
                      id:
                      {{id}}
                      <br />
                      name:
                      {{name}}
                      <br />
                      gender:
                      {{gender}}
                      <br />
                      age:
                      {{age}}
                      <br />
                      <br />
                      <h6>Mutual friends</h6>
                      <ul>
                        {{#each mutual_friends}}
                          <li>
                            id:
                            {{id}}
                            <br />
                            name:
                            {{name}}
                            <br />
                            gender:
                            {{gender}}
                            <br />
                            age:
                            {{age}}
                            <br />
                            <br />
                            {{#if mutual_friends}}
                              <h6>Mutual friends</h6>
                              <ul>
                                {{#each mutual_friends}}
                                  <li>
                                    id:
                                    {{id}}
                                    <br />
                                    name:
                                    {{name}}
                                    <br />
                                    gender:
                                    {{gender}}
                                    <br />
                                    age:
                                    {{age}}
                                    <br />
                                    <br />
                                  </li>
                                {{/each}}
                              </ul>
                            {{/if}}
                          </li>
                        {{/each}}
                      </ul>
                    </li>
                    {{#if groups}}
                      <h5>Groups:</h5>
                      <ul>
                        {{#each groups}}
                          <li>
                            id:
                            {{id}}
                            <br />
                            name:
                            {{name}}
                            <br />
                            group about:
                            {{description}}
                            <br />
                            <br />
                            <h6>Members:</h6>
                            <ol>
                              {{#each members}}
                                <li>
                                  id:
                                  {{id}}
                                  <br />
                                  name:
                                  {{name}}
                                  <br />
                                  gender:
                                  {{gender}}
                                  <br />
                                  age:
                                  {{age}}
                                  <br />
                                </li>
                              {{/each}}
                            </ol>
                          </li>
                        {{/each}}
                      </ul>
                    {{/if}}
                  {{/each}}
                </ol>
              </li>
              {{#if groups}}
                <h5>Groups:</h5>
                <ul>
                  {{#each groups}}
                    <li>
                      id:
                      {{id}}
                      <br />
                      name:
                      {{name}}
                      <br />
                      group about:
                      {{description}}.
                      <br />
                      <br />
                      <h6>Members:</h6>
                      <ol>
                        {{#each members}}
                          <li>
                            id:
                            {{id}}
                            <br />
                            name:
                            {{name}}
                            <br />
                            gender:
                            {{gender}}
                            <br />
                            age:
                            {{age}}
                            <br />
                          </li>
                        {{/each}}
                      </ol>
                    </li>
                  {{/each}}
                </ul>
              {{/if}}
              <hr />
            </div>
          {{/each}}
        </div>
      </ol>
    </section>
  </main>
</div>