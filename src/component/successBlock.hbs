{{#with service}}
  <div class='card mb-5 text-bg-success text-center'>
    <div class='card-body p-2'>
      <h2 class='card-title'>{{title}}</h2>
      <p>{{description}}</p>
      {{#each buttons}}
        <a
          href='{{link}}'
          class='btn btn-primary'
        >{{text}}</a>
      {{/each}}
    </div>
  </div>
{{/with}}